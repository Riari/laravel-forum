{
    "name": "riari/laravel-forum",
    "description": "A Laravel 6 package providing a solid foundation for building discussion forums",
    "keywords": ["laravel", "laravel-6", "forum", "discuss"],
    "license": "MIT",
    "authors": [
        {
            "name": "Riari",
            "email": "riari@teamteatime.net"
        },
        {
            "name": "NeeHi",
            "email": "neehi@teamteatime.net"
        }
    ],
    "require": {
        "php": ">7.2",
        "laravel/framework": "6 - 8",
        "kalnoy/nestedset": "^5.0",
        "doctrine/dbal": "^2.12"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "TeamTeaTime\\Forum\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TeamTeaTime\\Forum\\Database\\Factories\\": "database/factories"
        }
    },
    "extra": {
    "branch-alias": {
        "dev-master": "5.0.x-dev"
      }
    }
}
